+int get_bit_Rate(int value){
+    char x_int = value;
+    if(x_int < 64){
+        x_int = x_int;
+    }else if(x_int < 128){
+       x_int = (x_int-64)*8 + 64;
+    }else{
+        if(x_int == 255){
+            x_int = 0;
+        }else{
+            x_int = (x_int-128)*64 + 576;
+        }
+    }
+return x_int;
+}
